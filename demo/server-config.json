{
  "name": "demo-service",
  "version": "0.1.0",
  "maven": {
    "project": "-",
    "setting": "-"
  },
  "ant": {
    "task": "default"
  },
  "command": {
    "timeout": 0,
    "asadmin": {
      "win32": "{server.home}/bin/asadmin.bat",
      "unix": "{server.home}/bin/asadmin"
    },
    "maven": {
      "win32": "{maven.home}/bin/mvn.cmd",
      "unix": "{maven.home}/bin/mvn"
    },
    "ant": {
      "win32": "{ant.home}/bin/ant.bat",
      "unix": "{ant.home}/bin/ant"
    }
  },
  "domain": {
    "name": "demo",
    "home": "{server.domain.home}/domains",
    "deploy": {},
    "jdbc": {
      "JDBC-Name": {
        "dataSourceClassName": "com.mysql.jdbc.Driver",
        "restype": "javax.sql.DataSource",
        "properties": {
          "user": "{database.user}",
          "password": "{database.password}",
          "url": "jdbc:mysql://localhost:{database.port}/test"
        },
        "description": "This is a Test Connection"
      }
    },
    "ports": {
      "base": 48000
    }
  },
  "settings": {
    "server": {
      "home": "-"
    },
    "maven": {
      "home": "-"
    },
    "ant": {
      "home": "-"
    },
    "database": {
      "user": "myuser",
      "password": "mypassword",
      "port": 3306
    }
  },
  "plugins": {
    "serve": {
      "name": "serve-pipeline",
      "description": "Starts the server and deploy the application",
      "path": "hf/plugins/plugin-pipeline.js",
      "help": "tools/help/serve-help.txt",
      "settings": {
        "pipeline": [
          {
            "action": "start-server",
            "description": "Starts the Payara Glassfish Server",
            "params": [
              "-v"
            ]
          },
          {
            "action": "deploy-maven",
            "description": "Creates a clean package",
            "params": [
              "-v", "--clean"
            ]
          }
        ]
      }
    },
    "help": {
      "name": "help",
      "description": "The action overview of the project",
      "path": "hf/plugins/plugin-help.js",
      "help": "tools/help/plugin-help.txt"
    },
    "staging": {
      "name": "mystaging",
      "description": "",
      "path": "tools/plugins/plugin-staging.js",
      "help": "tools/help/plugin-staging.txt"
    }
  },
  "env": {
    "project.home": "{project.home}",
    "user.home": "{user.home}",
    "project.name": "{project.name}",
    "project.version": "{project.version}"
  }
}
