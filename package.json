{
  "name": "heringsfish-cli",
  "title": "Heringsfish CLI",
  "version": "1.0.0",
  "description": "Command Line Interface to manages the Glassfish / Payara Application Server and the deployment on local or server machine.",
  "author": "BlueSkyFish <blueskyfish@blueskyfish.de>",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "glassfish",
    "payara",
    "application server",
    "jee",
    "jee7",
    "management console",
    "admin"
  ],
  "bin": {
    "zf": "zfish.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "./node_modules/.bin/mocha",
    "website": "./node_modules/.bin/trash ./docs/* && ./node_modules/.bin/generate-md --layout ./website/layout --input ./website/content --output ./docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blueskyfish/heringsfish-cli.git"
  },
  "dependencies": {
    "app-module-path": "2.2.0",
    "colors": "1.1.2",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "moment": "2.17.1",
    "q": "1.4.1"
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "gulp-connect": "5.0.0",
    "gulp-util": "3.0.8",
    "markdown-styles": "3.1.9",
    "mocha": "3.2.0",
    "trash-cli": "1.4.0"
  }
}
